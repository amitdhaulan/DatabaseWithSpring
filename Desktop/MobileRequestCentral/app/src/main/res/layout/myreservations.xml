<?xml version="1.0" encoding="utf-8"?>
<FrameLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent" android:layout_height="match_parent"
    android:orientation="horizontal"
    android:weightSum="1">




    <LinearLayout
        android:id="@+id/back"
        android:tag="back"
        android:weightSum="1"
        android:orientation="horizontal"
        style="@style/MyListBackContent" >

       <!-- <Button
            android:layout_width="0dp"
            android:layout_weight=".22"
            android:layout_height="wrap_content"
            android:layout_gravity="center"
            android:id="@+id/swipe_button1"
            android:textColor="#ffffff"
            android:textSize="14sp"
            android:textStyle="bold"
            android:background="@drawable/buttoncustom"
            style="@style/MyListButtonAction"
            android:text="Check In"/>-->


        <Button
            android:layout_width="0dp"
            android:layout_weight=".22"
            android:layout_height="wrap_content"
            android:layout_gravity="center"
            android:id="@+id/swipe_button2"
            android:background="@drawable/buttoncustom"
            android:textColor="#ffffff"
            android:textSize="14sp"
            android:textStyle="bold"
            style="@style/MyListButtonAction"
            android:text="Cancel"/>

    </LinearLayout>



    <LinearLayout


        android:orientation="vertical"
        android:layout_height="match_parent"
        android:layout_width="match_parent">

        <TextView
            android:id="@+id/list_header"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:gravity="center_vertical"
            android:paddingLeft="30dp"
            android:layout_gravity="center"
            android:background="#FF9EA7FF"
            android:layout_weight=".05"
            android:textStyle="bold"
            android:text="15:15:00"/>


        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="0dp"
            android:weightSum="1"
            android:id="@+id/front"
            android:tag="front"
            android:orientation="horizontal"
            style="@style/MyListFrontContent"
            android:layout_marginLeft="5dp"
            android:layout_marginRight="5dp"
            android:layout_weight=".95">


            <LinearLayout
                android:layout_width="0dp"
                android:layout_height="match_parent"
                android:weightSum="1.2"
                android:orientation="vertical"
                android:layout_margin="5dp"
                android:layout_weight=".9">


                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="0dp"
                    android:weightSum="1"
                    android:gravity="center|center_vertical"
                    android:orientation="horizontal"
                    android:layout_gravity="center|center_vertical"
                    android:layout_weight=".2">

                    <TextView
                        android:layout_width="0dp"
                        android:layout_height="match_parent"
                        android:gravity="left|center_vertical"
                        android:layout_gravity="center"
                        android:layout_weight=".35"
                        android:text="@string/date"
                        android:textSize="14dp"
                        android:textStyle="bold"
                        android:textIsSelectable="false"
                        android:focusableInTouchMode="true" />

                    <TextView
                        android:layout_width="wrap_content"
                        android:text=":"
                        android:textSize="14dp"
                        android:gravity="left|center_vertical"
                        android:layout_gravity="center"
                        android:textStyle="bold"
                        android:layout_height="wrap_content" />

                    <TextView
                        android:id="@+id/date"
                        android:layout_width="0dp"
                        android:layout_height="match_parent"
                        android:gravity="left|center_vertical"
                        android:layout_marginLeft="5dp"
                        android:layout_gravity="center"
                        android:layout_weight=".6"
                        android:text="12/08/2015"/>

                </LinearLayout>

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="0dp"
                    android:weightSum="1"
                    android:gravity="center|center_vertical"
                    android:orientation="horizontal"
                    android:layout_gravity="center|center_vertical"
                    android:layout_weight=".2">

                    <TextView
                        android:layout_width="0dp"
                        android:layout_height="match_parent"
                        android:gravity="left|center_vertical"
                        android:textSize="14dp"
                        android:textStyle="bold"
                        android:layout_gravity="center"
                        android:layout_weight=".35"
                        android:text="@string/starttime"/>

                    <TextView
                        android:layout_width="wrap_content"
                        android:text=":"
                        android:textSize="14dp"
                        android:gravity="left|center_vertical"
                        android:layout_gravity="center"
                        android:textStyle="bold"
                        android:layout_height="wrap_content" />

                    <TextView
                        android:id="@+id/starttime"
                        android:layout_width="0dp"
                        android:layout_height="match_parent"
                        android:gravity="left|center_vertical"
                        android:layout_marginLeft="5dp"
                        android:layout_gravity="center"
                        android:layout_weight=".6"
                        android:text="15:15:00"/>

                </LinearLayout>

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="0dp"
                    android:weightSum="1"
                    android:gravity="center|center_vertical"
                    android:orientation="horizontal"
                    android:layout_gravity="center|center_vertical"
                    android:layout_weight=".2">

                    <TextView
                        android:layout_width="0dp"
                        android:layout_height="match_parent"
                        android:gravity="left|center_vertical"
                        android:textSize="14dp"
                        android:textStyle="bold"
                        android:layout_gravity="center"
                        android:layout_weight=".35"
                        android:text="@string/endtime"/>


                    <TextView
                        android:layout_width="wrap_content"
                        android:text=":"
                        android:textSize="14dp"
                        android:gravity="left|center_vertical"
                        android:layout_gravity="center"
                        android:textStyle="bold"
                        android:layout_height="wrap_content" />

                    <TextView
                        android:id="@+id/endtime"
                        android:layout_width="0dp"
                        android:layout_height="match_parent"
                        android:gravity="left|center_vertical"
                        android:layout_marginLeft="5dp"
                        android:layout_gravity="center"
                        android:layout_weight=".6"
                        android:text="16:15:00"/>

                </LinearLayout>

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="0dp"
                    android:weightSum="1"
                    android:gravity="center|center_vertical"
                    android:orientation="horizontal"
                    android:layout_gravity="center|center_vertical"
                    android:layout_weight=".2">

                    <TextView
                        android:layout_width="0dp"
                        android:layout_height="match_parent"
                        android:gravity="left|center_vertical"
                        android:textSize="14dp"
                        android:textStyle="bold"
                        android:layout_gravity="center"
                        android:layout_weight=".35"
                        android:text="@string/building"/>

                    <TextView
                        android:layout_width="wrap_content"
                        android:text=":"
                        android:textSize="14dp"
                        android:gravity="left|center_vertical"
                        android:layout_gravity="center"
                        android:textStyle="bold"
                        android:layout_height="wrap_content" />

                    <TextView
                        android:id="@+id/building"
                        android:layout_width="0dp"
                        android:layout_height="match_parent"
                        android:gravity="left|center_vertical"
                        android:layout_marginLeft="5dp"
                        android:layout_gravity="center"
                        android:layout_weight=".6"
                        android:text="1421 Peachtree Street"/>

                </LinearLayout>

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="0dp"
                    android:weightSum="1"
                    android:gravity="center|center_vertical"
                    android:orientation="horizontal"
                    android:layout_gravity="center|center_vertical"
                    android:layout_weight=".2">

                    <TextView
                        android:layout_width="0dp"
                        android:layout_height="match_parent"
                        android:gravity="left|center_vertical"
                        android:textSize="14dp"
                        android:textStyle="bold"
                        android:layout_gravity="center"
                        android:layout_weight=".35"
                        android:text="@string/floor"/>

                    <TextView
                        android:layout_width="wrap_content"
                        android:text=":"
                        android:textSize="14dp"
                        android:gravity="left|center_vertical"
                        android:layout_gravity="center"
                        android:textStyle="bold"
                        android:layout_height="wrap_content" />

                    <TextView
                        android:id="@+id/floor"
                        android:layout_width="0dp"
                        android:layout_height="match_parent"
                        android:gravity="left|center_vertical"
                        android:layout_marginLeft="5dp"
                        android:layout_gravity="center"
                        android:layout_weight=".6"
                        android:text="21-Twenty-First Floor"/>

                </LinearLayout>

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="0dp"
                    android:weightSum="1"
                    android:gravity="center|center_vertical"
                    android:orientation="horizontal"
                    android:layout_gravity="center|center_vertical"
                    android:layout_weight=".2">

                    <TextView
                        android:layout_width="0dp"
                        android:layout_height="match_parent"
                        android:gravity="left|center_vertical"
                        android:textSize="14dp"
                        android:textStyle="bold"
                        android:layout_gravity="center"
                        android:layout_weight=".35"
                        android:text="@string/space"/>

                    <TextView
                        android:layout_width="wrap_content"
                        android:text=":"
                        android:textSize="14dp"
                        android:gravity="left|center_vertical"
                        android:layout_gravity="center"
                        android:textStyle="bold"
                        android:layout_height="wrap_content" />

                    <TextView
                        android:id="@+id/space"
                        android:layout_width="0dp"
                        android:layout_height="match_parent"
                        android:gravity="left|center_vertical"
                        android:layout_marginLeft="5dp"
                        android:layout_gravity="center"
                        android:layout_weight=".6"
                        android:text="1730"/>

                </LinearLayout>

            </LinearLayout>

            <LinearLayout
                android:layout_width="0dp"
                android:layout_height="match_parent"
                android:weightSum="1"
                android:orientation="horizontal"
                android:gravity="center"
                android:layout_weight=".1">

                <CheckBox
                    android:id="@+id/reservationcheckbox"
                    android:layout_width="0dp"
                    android:layout_height="wrap_content"
                    android:focusable="false"
                    android:focusableInTouchMode="false"
                    android:layout_weight="1"
                    android:layout_gravity="center"/>


                <com.applidium.shutterbug.FetchableImageView
                    android:id="@+id/checkedintag"
                    android:background="@drawable/checkedin"
                    android:visibility="gone"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"/>

            </LinearLayout>

        </LinearLayout>
    </LinearLayout>
</FrameLayout>